{
  "name": "multimind-agent-platform",
  "version": "0.1.45",
  "main": "dist/main.electron.js",
  "scripts": {
    "uuid": "tsx src/cli/generate-uuid.ts",
    "test": "jest",
    "dev": "yarn electron:dev",
    "build": "yarn build:app",
    "package": "yarn electron:package",
    "docs": "mkdocs serve",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate-icon": "node scripts/generate-icon.js",
    "generate-icon:dev": "node scripts/generate-icon.js --dev",
    "release": "electron-builder --publish always --config ./electron-builder.ts",
    "schemas": "ts-json-schema-generator --path 'src/schemas/**/*.ts' --type '*' -o src/schemas/schema.json --no-type-check",
    "watch:schemas": "chokidar 'src/schemas/**/*.ts' -c 'yarn schemas'",
    "start": "yarn electron:start:dev",
    "start:webmode": "concurrently \"npm run start:server\" \"npm run start:web\"",
    "start:server": "tsx src/main.webserver.ts",
    "start:web": "webpack serve --mode development",
    "clean": "rimraf dist",
    "download-whisper-models": "node scripts/download-whisper-models.js",
    "build:clean": "yarn clean && yarn build:app",
    "build:app": "concurrently \"yarn generate-icon\" \"yarn build:weblibs\" \"yarn build:worker\" \"yarn build:electron\" \"yarn build:preload\" \"yarn build:web\"",
    "build:preload": "webpack --config webpack.preload.config.js --mode production",
    "build:electron": "webpack --config webpack.backend.config.js --mode production",
    "build:web": "webpack --config webpack.config.js --mode production",
    "build:worker": "webpack --config webpack.worker.config.js",
    "build:weblibs": "webpack --config webpack.website-libs.config.js",
    "watch": "concurrently \"yarn watch:app\" \"yarn watch:schemas\"",
    "watch:webmode": "concurrently \"npm run watch:server\" \"npm run start:web\"",
    "watch:web": "webpack --config webpack.config.js --mode development --watch",
    "watch:server": "concurrently \"webpack --config webpack.backend.config.js --mode development --watch\" \"webpack --config webpack.preload.config.js --mode development --watch\"",
    "watch:worker": "webpack --config webpack.worker.config.js --watch --mode development",
    "watch:app": "concurrently \"npm run watch:web\" \"npm run watch:server\" \"npm run watch:worker\"",
    "electron:dev": "concurrently \"npm run watch:web\" \"npm run watch:server\" \"yarn watch:schemas\" \"yarn watch:worker\" \"wait-on dist/main.electron.js dist/web/bundle.js && yarn electron:start:dev\"",
    "electron:start:dev": "nodemon --watch 'dist/main.electron.js' --delay 3 --exec \"electron . --dev-mode --inspect\"",
    "electron:build": "yarn download-whisper-models && electron-builder --config ./electron-builder.ts",
    "electron:package": "yarn build:clean && yarn electron:build",
    "electron:package:all": "electron-builder --config ./electron-builder.ts build --mac --win --linux",
    "electron:deploy:linux": "electron-builder --config ./electron-builder.ts build --linux --publish always",
    "postinstall": "electron-builder install-app-deps"
  },
